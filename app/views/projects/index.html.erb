<%= javascript_pack_tag 'project' %>

<% content_for :card_header do %>
    <div class="card-header">
      <h2 class="font-weight-bold float-left">Projects</h2>
      <% if can? :create, Project %>
        <%= button_to "Create Projects", new_project_path, method: 'get', class: "btn btn-dark float-right mt-1" %>
      <% end %>
    </div>
<% end %>

<div class="table-responsive" id="table-parent-div">
  <%= render partial: 'table', locals: { projects: @projects } %>
</div>

<div class="digg_pagination">
  <%= will_paginate @projects %>
</div>
